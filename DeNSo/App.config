<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="DataBasePath" value="c:\densodb"/>
    <add key="EnableJournaling" value="true"/>
    <add key="EnableOperationsLog" value="true"/>
    <add key="IndexBasePath" value="c:\densodb\indexes"/>
  </appSettings>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true"/>
                    <serviceDebug includeExceptionDetailInFaults="false"/>
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service name="DeNSo.dbservice">
                <endpoint address="" binding="wsHttpBinding" contract="DeNSo.dbservice">
                    <identity>
                        <!--<dns value="localhost"/>-->
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8732/dbservice/"/>
                    </baseAddresses>
                </host>
            </service>
            <service name="DeNSo.restservice">
                <endpoint address="" binding="wsHttpBinding" contract="DeNSo.restservice">
                    <identity>
                        <!--<dns value="localhost"/>-->
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8732/restservice/"/>
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
